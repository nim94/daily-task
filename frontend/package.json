    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash.debounce": "^4.0.8",
    "next": "^13.0.2",
    "next-cookies": "^1.1.2",
    "next-redux-wrapper": "^3.0.0-alpha.2",
